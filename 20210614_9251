#LCS
def lcs(s1,s2):
  n1 = len(s1)
  n2 = len(s2)
  #2차원 리스트생성
  s = [[0]*(n2+1) for i in range(n1+1)]
  
  #lcs
  for i in range(1,n1+1):
    for j in range(1,n2+1):
      if s1[i-1] == s2[j-1]:
        s[i][j] = s[i-1][j-1] + 1
      else:
        s[i][j] = max(s[i-1][j],s[i][j-1])
  #출력
  print(s[-1][-1])

#입력
s1 = input()
s2 = input()
lcs(s1,s2)
